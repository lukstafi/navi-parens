{
  "name": "navi-parens",
  "publisher": "lukstafi",
  "displayName": "Navi Parens",
  "description": "Navigate structured code with convenient key shortcuts.",
  "icon": "iconSmall.png",
  "version": "2.0.0",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "structure",
    "navigation",
    "block",
    "jump",
    "selection",
    "WYSIWYG",
    "latex"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukstafi/navi-parens"
  },
  "bugs": {
    "url": "https://github.com/lukstafi/navi-parens/issues"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "navi-parens.goPastNextScope",
        "title": "Go To Next Scope: Go past the next same-level closing bracket/scope",
        "shortTitle": "Go To Next Scope"
      },
      {
        "command": "navi-parens.goPastPreviousScope",
        "title": "Go To Previous Scope: Go to the previous same-level opening bracket/scope",
        "shortTitle": "Go To Previous Scope"
      },
      {
        "command": "navi-parens.goToUpScope",
        "title": "Go To Up Scope: Go outside the opening of the current level",
        "shortTitle": "Go To Up Scope"
      },
      {
        "command": "navi-parens.goToDownScope",
        "title": "Go To Down Scope: Go outside the closing of the current level",
        "shortTitle": "Go To Down Scope"
      },
      {
        "command": "navi-parens.goToBeginScope",
        "title": "Go To Begin Scope: Go near the opening of the current level but stay inside scope",
        "shortTitle": "Go To Begin Scope"
      },
      {
        "command": "navi-parens.goToEndScope",
        "title": "Go To End Scope: Go near the closing of the current level but stay inside scope",
        "shortTitle": "Go To End Scope"
      },
      {
        "command": "navi-parens.goToPreviousEmptyLine",
        "title": "Go To Previous Empty Line: Go to the previous line with only whitespace (or empty)",
        "shortTitle": "Go To Previous Empty Line"
      },
      {
        "command": "navi-parens.goToNextEmptyLine",
        "title": "Go To Next Empty Line: Go to the next line with only whitespace (or empty)",
        "shortTitle": "Go To Next Empty Line"
      },
      {
        "command": "navi-parens.goPastPreviousWord",
        "title": "Go To Previous Word: Go to the beginning of the current/previous word",
        "shortTitle": "Go To Previous Word"
      },
      {
        "command": "navi-parens.goPastNextWord",
        "title": "Go To Next Word: Go to the end of the current/next word",
        "shortTitle": "Go To Next Word"
      },
      {
        "command": "navi-parens.selectPastNextScope",
        "title": "Select To Next Scope: Select past the next same-level closing bracket/scope",
        "shortTitle": "Select To Next Scope"
      },
      {
        "command": "navi-parens.selectPastPreviousScope",
        "title": "Select To Previous Scope: Select to the previous same-level opening bracket/scope",
        "shortTitle": "Select To Previous Scope"
      },
      {
        "command": "navi-parens.selectToUpScope",
        "title": "Slect To Up Scope: Select till outside the opening of the current level",
        "shortTitle": "Slect To Up Scope"
      },
      {
        "command": "navi-parens.selectToDownScope",
        "title": "Select To Down Scope: Select till outside the closing of the current level",
        "shortTitle": "Select To Down Scope"
      },
      {
        "command": "navi-parens.selectToBeginScope",
        "title": "Slect To Begin Scope: Select to near the opening of the current level but stay inside scope",
        "shortTitle": "Slect To Begin Scope"
      },
      {
        "command": "navi-parens.selectToEndScope",
        "title": "Select To End Scope: Select to near the closing of the current level but stay inside scope",
        "shortTitle": "Select To End Scope"
      },
      {
        "command": "navi-parens.selectToPreviousEmptyLine",
        "title": "Select To Previous Empty Line: Select to the previous line with only whitespace (or empty)",
        "shortTitle": "Select To Previous Empty Line"
      },
      {
        "command": "navi-parens.selectToNextEmptyLine",
        "title": "Select To Next Empty Line: Select to the next line with only whitespace (or empty)",
        "shortTitle": "Select To Next Empty Line"
      },
      {
        "command": "navi-parens.selectPastPreviousWord",
        "title": "Select To Previous Word: select to the beginning of the previous word",
        "shortTitle": "Select To Previous Word"
      },
      {
        "command": "navi-parens.selectPastNextWord",
        "title": "Select To Next Word: select to the end of the next word",
        "shortTitle": "Select To Next Word"
      },
      {
        "command": "navi-parens.cycleBracketScopeMode",
        "title": "Cycle Bracket Scope Mode: switch the algorithm navigating delimiters",
        "shortTitle": "Cycle Bracket Scope Mode"
      },
      {
        "command": "navi-parens.cycleBlockScopeMode",
        "title": "Cycle Block Scope Mode: switch the algorithm navigating code blocks",
        "shortTitle": "Cycle Block Scope Mode"
      },
      {
        "command": "navi-parens.toggleMarkmacsMode",
        "title": "Toggle Markmacs Mode: Turn on / off cursor and scope visualization",
        "shortTitle": "Toggle Markmacs Mode"
      },
      {
        "command": "navi-parens.goRightOrSiblingScope",
        "title": "Go right one character/separator, or to a sibling scope on the right",
        "shortTitle": "Go Right Or Sibling Scope"
      },
      {
        "command": "navi-parens.goLeftOrSiblingScope",
        "title": "Go left one character/separator, or to a sibling scope on the left",
        "shortTitle": "Go Left Or Sibling Scope"
      },
      {
        "command": "navi-parens.goUpOneLiner",
        "title": "Go left / up at least `oneLinerMin` characters, stopping on a scope boundary",
        "shortTitle": "Go Up One Liner"
      },
      {
        "command": "navi-parens.goDownOneLiner",
        "title": "Go right / down at least `oneLinerMin` characters, stopping on a scope boundary",
        "shortTitle": "Go Down One Liner"
      },
      {
        "command": "navi-parens.goBeginScopeOrArg",
        "title": "Go near the opening of the current scope or argument",
        "shortTitle": "Go Begin Scope Or Arg"
      },
      {
        "command": "navi-parens.goEndScopeOrArg",
        "title": "Go near the closing of the current scope or argument",
        "shortTitle": "Go End Scope Or Arg"
      },
      {
        "command": "navi-parens.selectRightOrSiblingScope",
        "title": "Like `goRightOrSiblingScope`, but extend the selection",
        "shortTitle": "Select Right Or Sibling Scope"
      },
      {
        "command": "navi-parens.selectLeftOrSiblingScope",
        "title": "Like `goLeftOrSiblingScope`, but extend the selection",
        "shortTitle": "Select Left Or Sibling Scope"
      },
      {
        "command": "navi-parens.selectUpOneLiner",
        "title": "Like `goUpOneLiner`, but extend the selection",
        "shortTitle": "Select Up One Liner"
      },
      {
        "command": "navi-parens.selectDownOneLiner",
        "title": "Like `goDownOneLiner`, but extend the selection",
        "shortTitle": "Select Down One Liner"
      },
      {
        "command": "navi-parens.selectBeginScopeOrArg",
        "title": "Like `goBeginScopeOrArg`, but extend the selection",
        "shortTitle": "Select Begin Scope Or Arg"
      },
      {
        "command": "navi-parens.selectEndScopeOrArg",
        "title": "Like `goEndScopeOrArg`, but extend the selection",
        "shortTitle": "Select End Scope Or Arg"
      }
    ],
    "configuration": {
      "title": "Navi Parens",
      "properties": {
        "navi-parens.rebind": {
          "markdownDescription": "How to deal with the `shift+alt+i` binding conflict. If `true`, the built-in command becomes `shift+alt+p`. If `false`, we add navigation to `ctrl+alt+o` instead of `alt+i`",
          "type": "boolean",
          "default": "true",
          "scope": "application"
        },
        "navi-parens.blockScopeMode": {
          "markdownDescription": "Where the non-bracket structure information comes from. If `Semantic`, uses VSCode symbol providers. If `Indentation`, generates symbols from indentation. If `None`, there are no symbols",
          "type": "string",
          "enum": [
            "Semantic",
            "Indentation",
            "None"
          ],
          "default": "Semantic",
          "scope": "language-overridable"
        },
        "navi-parens.bracketScopeMode": {
          "markdownDescription": "What algorithm decides on bracket-based navigation. If `JumpToBracket`, uses the built-in `ctrl+shift+\\` command. If `Raw`, matches any of the opening with any of the closing brackets disregarding other context. If `None`, there is no bracket-based navigation",
          "type": "string",
          "enum": [
            "JumpToBracket",
            "Raw",
            "None"
          ],
          "default": "JumpToBracket",
          "scope": "language-overridable"
        },
        "navi-parens.isMarkmacsMode": {
          "markdownDescription": "Markmacs Mode enabled/disabled: whether to track cursor position",
          "type": "boolean",
          "default": "false",
          "scope": "language-overridable"
        },
        "navi-parens.closingBrackets": {
          "description": "The superset of supported closing delimiters (the default should be enough)",
          "type": "array",
          "default": [
            ")",
            "]",
            "}",
            ">"
          ],
          "scope": "language-overridable"
        },
        "navi-parens.openingBrackets": {
          "description": "The superset of supported opening delimiters (the default should be enough)",
          "type": "array",
          "default": [
            "(",
            "[",
            "{",
            "<"
          ],
          "scope": "language-overridable"
        },
        "navi-parens.separatorsNoMM": {
          "markdownDescription": "The separators for commands like `goBeginScopeOrArg`, `goUpOneLiner` when not in Markmacs Mode",
          "type": "array",
          "default": [
            ",",
            ";",
            "&",
            "\\\\",
            "}{"
          ],
          "scope": "language-overridable"
        },
        "navi-parens.closingBracketsRaw": {
          "markdownDescription": "The closing delimiters for the `Raw` `bracketScopeMode`",
          "type": "array",
          "default": [
            "\\end{pmatrix}",
            "\\end{bmatrix}",
            "\\end{pmatrix}",
            "\\end{vmatrix}",
            "\\end{Vmatrix}",
            "\\end{Bmatrix}",
            "\\end{matrix}",
            "\\end{rcases}",
            "\\end{cases}",
            "\\end{smallmatrix}",
            "\\end{equation*}",
            "\\end{equation}",
            "\\end{split}",
            "\\end{gather*}",
            "\\end{gathered}",
            "\\end{gather}",
            "\\end{alignat*}",
            "\\end{alignedat}",
            "\\end{alignat}",
            "\\end{align*}",
            "\\end{aligned}",
            "\\end{align}",
            "\\end{CD}",
            " *)",
            ")",
            "|]",
            "]",
            "}",
            "</p>",
            "</div>"
          ],
          "scope": "language-overridable"
        },
        "navi-parens.openingBracketsRaw": {
          "markdownDescription": "The closing delimiters for the `Raw` `bracketScopeMode`",
          "type": "array",
          "default": [
            "\\begin{pmatrix}",
            "\\begin{bmatrix}",
            "\\begin{pmatrix}",
            "\\begin{vmatrix}",
            "\\begin{Vmatrix}",
            "\\begin{Bmatrix}",
            "\\begin{matrix}",
            "\\begin{rcases}",
            "\\begin{cases}",
            "\\begin{smallmatrix}",
            "\\begin{equation*}",
            "\\begin{equation}",
            "\\begin{split}",
            "\\begin{gather*}",
            "\\begin{gathered}",
            "\\begin{gather}",
            "\\begin{alignat*}",
            "\\begin{alignedat}",
            "\\begin{alignat}",
            "\\begin{align*}",
            "\\begin{aligned}",
            "\\begin{align}",
            "\\begin{CD}",
            "/\\\\[a-zA-Z0-9]*\\{/",
            "(** ",
            "(* ",
            "(",
            "[|",
            "[",
            "{",
            "<p>",
            "<div"
          ],
          "scope": "language-overridable"
        },
        "navi-parens.separatorsMM": {
          "markdownDescription": "The separators for commands like `goBeginScopeOrArg`, `goUpOneLiner` when in the Markmacs Mode",
          "type": "array",
          "default": [
            "\\\\",
            "&",
            "}{"
          ],
          "scope": "language-overridable"
        },
        "navi-parens.pastWordRegex": {
          "markdownDescription": "The regexp for the commands `goPastNextWord`, `goPastPreviousWord`",
          "type": "string",
          "default": "\\p{General_Category=Letter}|[0-9]|_",
          "scope": "language-overridable"
        },
        "navi-parens.pseudoSeparatorNoMM": {
          "markdownDescription": "A regexp for additional separators to skip in the commands `goRightOrSiblingScope`, `goLeftOrSiblingScope` when not in Markmacs Mode",
          "type": "string",
          "default": "\\\\([a-zA-Z0-9]+|[^{])",
          "scope": "language-overridable"
        },
        "navi-parens.pseudoSeparatorMM": {
          "markdownDescription": "A regexp for additional separators to skip in the commands `goRightOrSiblingScope`, `goLeftOrSiblingScope` in Markmacs Mode",
          "type": "string",
          "default": "\\\\([a-zA-Z0-9]+|[^{])",
          "scope": "language-overridable"
        },
        "navi-parens.oneLinerMin": {
          "markdownDescription": "The minimal offset to traverse for the commands `goUpOneLiner`, `goDownOneLiner`",
          "type": "number",
          "default": "30",
          "scope": "language-overridable"
        }
      }
    },
    "keybindings": [
      {
        "command": "-editor.action.insertCursorAtEndOfEachLineSelected",
        "key": "shift+alt+i",
        "when": "editorTextFocus && config.navi-parens.rebind"
      },
      {
        "command": "editor.action.insertCursorAtEndOfEachLineSelected",
        "key": "shift+alt+p",
        "when": "editorTextFocus && config.navi-parens.rebind"
      },
      {
        "command": "cursorRight",
        "key": "alt+l",
        "when": "textInputFocus && !editorTextFocus"
      },
      {
        "command": "cursorLeft",
        "key": "alt+j",
        "when": "textInputFocus && !editorTextFocus"
      },
      {
        "command": "cursorUp",
        "key": "alt+i",
        "when": "textInputFocus && !editorTextFocus"
      },
      {
        "command": "list.focusUp",
        "key": "alt+i",
        "when": "listFocus && !inputFocus"
      },
      {
        "command": "selectPrevCodeAction",
        "key": "alt+i",
        "when": "codeActionMenuVisible"
      },
      {
        "command": "selectPrevSuggestion",
        "key": "alt+i",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
      },
      {
        "command": "showPrevParameterHint",
        "key": "alt+i",
        "when": "editorFocus && parameterHintsMultipleSignatures && parameterHintsVisible"
      },
      {
        "command": "cursorUp",
        "key": "ctrl+alt+o",
        "when": "textInputFocus && !config.navi-parens.rebind"
      },
      {
        "command": "cursorDown",
        "key": "alt+k",
