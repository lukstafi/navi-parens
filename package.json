        "command": "cursorRightSelect",
        "key": "shift+alt+l",
        "when": "textInputFocus && !editorTextFocus"
      },
      {
        "command": "cursorLeftSelect",
        "key": "shift+alt+j",
        "when": "textInputFocus && !editorTextFocus"
      },
      {
        "command": "cursorUpSelect",
        "key": "shift+alt+i",
